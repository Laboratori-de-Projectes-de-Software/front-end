{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@styles/*": ["src/styles/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@components/*": ["src/components/*"],
    }
  }
}